{
  "version": 2,
  "buildCommand": "Nestjs-server-learn",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@now/node"
    }
  ],
  "rewrites": [
    { 
      "source": "/api/(.*)", 
      "destination": "dist/main.js" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}