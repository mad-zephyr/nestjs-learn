{
    "version": 2,
    "env": {
        "MONGO_URI": "@shop_api_mongo_uri",
        "MONGO_LOGIN":"admin",
        "MONGO_PASSWORD":"admin",
        "MONGO_HOST":"localhost",
        "MONGO_PORT":27017,
        "MONGO_AUTH_DB":"admin",
        "JWT_SECRET":"testSecret"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dist": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ]
}